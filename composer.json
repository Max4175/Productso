{
    "name": "Max4175/productso",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "Max4175",
        "productso"
    ],
    "homepage": "https://github.com/Max4175/productso",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "kalnoy/nestedset": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Max4175\\productso\\": "src/",
            "Max4175\\productso\\Http\\Controllers\\": "src/Http/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Max4175\\productso\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
